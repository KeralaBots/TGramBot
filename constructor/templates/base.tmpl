import logging
from pydantic import BaseModel

logger = logging.getLogger(__name__)


class TelegramObject(BaseModel):

    """
    Base TelegramObject Class for TGramBot
    """

    def __str__(self):
        return str(self.__dict__)

    def __repr__(self):
        return f"<{{type(self).__name__}} {{self}}>"
